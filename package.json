{
  "name": "yup-notebook",
  "displayName": "Yup Notebook",
  "description": "Notebook using Serializer API sample",
  "publisher": "x2nie",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:yup-notebook-serializer",
    "onCommand:yup-notebook.createJsonNotebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yup-notebook.createJsonNotebook",
        "title": "Create YUP Notebook"
      }
    ],
    "notebooks": [
      {
        "type": "yup-notebook-serializer",
        "displayName": "Yup DN Notebook",
        "selector": [
          {
            "filenamePattern": "*.sample-yup-notebook"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "lint": "eslint src --ext ts",
    "watch": "tsc -b --watch",
    "dev": "vite"
  },
  "keywords": [],
  "author": "Yuu6883",
  "license": "MIT",
  "devDependencies": {
    "@types/ace": "^0.0.48",
    "@types/file-saver": "^2.0.5",
    "@types/mocha": "^10.0.8",
    "@types/node": "14.x",
    "@types/seedrandom": "^3.0.2",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "@xmldom/xmldom": "^0.8.2",
    "ace-builds": "^1.8.1",
    "eslint": "^8.56.0",
    "gl-matrix": "^3.4.3",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "regl": "^2.1.0",
    "regl-atmosphere-envmap": "^1.0.5",
    "typescript": "^5.5.2",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@rgrove/parse-xml": "^4.1.0"
  }
}
